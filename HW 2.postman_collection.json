					"script": {
						"exec": [
							"let jsonData = pm.response.json();\r",
							"\r",
							"let user_name = jsonData.name;\r",
							"pm.test(\"Name is correct\", function(){\r",
							"    pm.expect(user_name).to.eql(\"Sergey\")\r",
							"});\r",
							"\r",
							"let user_age = +jsonData.age;\r",
							"pm.test(\"Age is correct\", function(){\r",
							"    pm.expect(user_age).to.eql(27)\r",
							"});\r",
							"\r",
							"let user_salary = +jsonData.salary;\r",
							"pm.test(\"Salary is correct\", function(){\r",
							"    pm.expect(user_salary).to.eql(3000)\r",
							"});\r",
							"\r",
							"console.log(\"Family contains =\",jsonData.family);\r",
							"\r",
							"let req = +pm.request.body.formdata.get(\"salary\");\r",
							"let sl15yr = +jsonData.family.u_salary_1_5_year;\r",
							"pm.test(\"Salary after 1.5 year is correct\", function(){\r",
